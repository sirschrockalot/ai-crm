# Test Mode Configuration
# Set TEST_MODE=true to enable test mode (bypasses authentication)
TEST_MODE=true

# Default test role (admin, acquisitions, dispositions)
TEST_DEFAULT_ROLE=admin

# JWT Configuration (still needed for test mode tokens)
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=24h

# Google OAuth (not used in test mode but required for module loading)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/dealcycle-test

# Server Configuration
PORT=3000
NODE_ENV=development 