{
  "environment": "production",
  "version": "1.0.0",
  "lastUpdated": "2024-12-19T00:00:00Z",
  
  "features": {
    "aiFeatures": {
      "enabled": true,
      "debugMode": false,
      "logLevel": "warn",
      "maxTokens": 4000,
      "temperature": 0.7
    },
    "automationFeatures": {
      "enabled": true,
      "debugMode": false,
      "maxWorkflows": 100,
      "executionTimeout": 300
    },
    "analytics": {
      "enabled": true,
      "trackingLevel": "standard",
      "debugMode": false,
      "sampleRate": 0.1
    },
    "notifications": {
      "enabled": true,
      "email": true,
      "push": true,
      "sms": true,
      "debugMode": false
    }
  },
  
  "security": {
    "mfa": {
      "enabled": true,
      "methods": ["totp", "sms"],
      "backupCodes": 10,
      "sessionTimeout": 3600
    },
    "passwordPolicy": {
      "minLength": 10,
      "requireUppercase": true,
      "requireLowercase": true,
      "requireNumbers": true,
      "requireSpecialChars": true,
      "maxAge": 90
    },
    "sessionManagement": {
      "maxConcurrentSessions": 3,
      "inactivityTimeout": 900,
      "absoluteTimeout": 43200
    }
  },
  
  "ui": {
    "theme": {
      "default": "light",
      "available": ["light", "dark", "auto"],
      "customColors": true
    },
    "dashboard": {
      "defaultLayout": "grid",
      "customizable": true,
      "maxWidgets": 20
    },
    "mobile": {
      "responsive": true,
      "touchOptimized": true,
      "offlineSupport": false
    }
  },
  
  "integrations": {
    "api": {
      "rateLimit": 200,
      "timeout": 30,
      "retryAttempts": 2,
      "debugMode": false
    },
    "thirdParty": {
      "googleWorkspace": {
        "enabled": false,
        "debugMode": false
      },
      "slack": {
        "enabled": false,
        "debugMode": false
      },
      "zapier": {
        "enabled": false,
        "debugMode": false
      }
    }
  },
  
  "workflows": {
    "maxSteps": 50,
    "executionEngine": "node",
    "debugMode": false,
    "logging": "minimal"
  },
  
  "customFields": {
    "maxFields": 100,
    "fieldTypes": ["text", "number", "date", "select", "multiselect", "boolean", "file"],
    "validation": true,
    "templates": true
  },
  
  "audit": {
    "enabled": true,
    "retentionDays": 2555,
    "logLevel": "warn",
    "exportFormats": ["json", "csv", "pdf"]
  },
  
  "performance": {
    "caching": {
      "enabled": true,
      "ttl": 1800,
      "maxSize": "500MB"
    },
    "optimization": {
      "lazyLoading": true,
      "codeSplitting": true,
      "imageOptimization": true
    }
  },
  
  "production": {
    "debugMode": false,
    "hotReload": false,
    "sourceMaps": false,
    "performanceMonitoring": true,
    "errorReporting": {
      "enabled": true,
      "level": "error",
      "captureUnhandled": true
    }
  }
}
